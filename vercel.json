{
    "version": 2,
    "routes": [
        { "src": "/api/auth/callback/github", "dest": "/api/auth.js" },
        { "src": "/api/github/save", "dest": "/api/github/save.js" },
        { "src": "/api/github/list", "dest": "/api/github/list.js" },
        { "handle": "filesystem" },
        { "src": "/lib/(.*)", "dest": "/lib/$1" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
